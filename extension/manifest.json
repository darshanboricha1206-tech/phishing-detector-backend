{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "2.0",
  "permissions": [
    "tabs",
    "notifications"
  ],
  "host_permissions": 
  [
  "https://phishing-detector.onrender.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}

